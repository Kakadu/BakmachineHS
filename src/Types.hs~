module Types (
  InterruptSort (IExit, IOutInt),
  Bytecmd,
  inter_of_int
) where

-- types for bytecode commands

data InterruptSort = 
    IExit
  | IOutInt deriving (Show)

data Bytecmd =
    Nop
  | Interrupt InterruptSort
  deriving (Show)


inter_of_int 10 = Just IExit
inter_of_int 11 = Just IOutInt
inter_of_int __ = Nothing
  